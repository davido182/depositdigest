{
  "name": "rentaflux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:mobile": "vite build --mode production && npx cap sync",
    "build:mobile:staging": "vite build --mode staging && npx cap sync",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "setup": "bash scripts/setup-env.sh",
    "security-check": "scripts/security-check-simple.bat",
    "security-check-full": "powershell -ExecutionPolicy Bypass -File scripts/security-check.ps1",
    "security-check-bash": "bash scripts/security-check.sh",
    "health-check": "node -e \"import('./src/utils/healthCheck.js').then(m => m.healthChecker.performHealthCheck()).then(console.log)\"",
    "mobile:build": "scripts/build-mobile.bat both production",
    "mobile:build:android": "scripts/build-mobile.bat android production",
    "mobile:build:ios": "scripts/build-mobile.bat ios production",
    "mobile:build:staging": "scripts/build-mobile.bat both staging",
    "mobile:dev:android": "npm run build:mobile && npx cap run android",
    "mobile:dev:ios": "npm run build:mobile && npx cap run ios",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:sync:staging": "npx cap sync --configuration staging",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "npx cap run ios",
    "cap:run:android": "npx cap run android",
    "cap:run:ios:staging": "npx cap run ios --configuration staging",
    "cap:run:android:staging": "npx cap run android --configuration staging",
    "deploy:staging": "npm run build:staging && echo 'Deploy to staging server'",
    "deploy:production": "npm run build && echo 'Deploy to production server'",
    "live-updates:setup": "scripts/setup-live-updates.bat",
    "live-updates:deploy": "scripts/deploy-with-live-updates.bat production",
    "live-updates:deploy:staging": "scripts/deploy-with-live-updates.bat staging",
    "build:release": "scripts/build-release-apk.bat",
    "setup:capacitor-cloud": "scripts/setup-capacitor-cloud.bat"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/live-updates": "^0.4.0",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@sentry/react": "^9.40.0",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.56.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.3.0",
    "framer-motion": "^12.23.3",
    "input-otp": "^1.2.4",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.462.0",
    "mysql2": "^3.9.1",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "lovable-tagger": "^1.1.7",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
